{
  "name": "d3-noglobal",
  "version": "3.5.3-2",
  "description": "A fork of d3 that is exactly the same but with no global by default if you load d3.js using a module systema and with no jsdom",
  "keywords": [
    "dom",
    "w3c",
    "visualization",
    "svg",
    "animation",
    "canvas"
  ],
  "homepage": "http://d3js.org",
  "author": {
    "name": "Mike Bostock",
    "url": "http://bost.ocks.org/mike"
  },
  "contributors": [
    {
      "name": "Jason Davies",
      "url": "http://jasondavies.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kileras/d3.git"
  },
  "main": "index.js",
  "browserify": "d3.js",
  "jspm": {
    "main": "d3",
    "shim": {
      "d3": {
        "exports": "d3"
      }
    },
    "files": [
      "d3.js"
    ],
    "buildConfig": {
      "uglify": true
    }
  },
  "jam": {
    "main": "d3.js",
    "shim": {
      "exports": "d3"
    }
  },
  "spm": {
    "main": "d3.js"
  },
  "dependencies": {},
  "devDependencies": {
    "smash": "~0.0.12",
    "uglify-js": "2.4.0",
    "vows": "0.7.0",
    "seedrandom": "2.3.1"
  },
  "scripts": {
    "test": "vows; echo"
  },
  "licenses": [
    {
      "type": "BSD",
      "url": "https://github.com/mbostock/d3/blob/master/LICENSE"
    }
  ]
}
